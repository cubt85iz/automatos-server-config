CONTAINER_PATH={{ path }}
{% if dataset is defined %}
DATASET={{ dataset }}
{% endif %}
{% if monitor_url is defined %}
MONITOR_URL={{ monitor_url }}
{% endif %}

{% if backup is defined %}
{% if backup.monitor_url is defined %}
BACKUP_MONITOR_URL={{ backup.monitor_url }}
{% endif %}
{% if backup.path is defined %}
BACKUP_PATH={{ backup.path }}
{% endif %}
{% if backup.remotes is defined %}
BACKUP_REMOTES=({{ backup.remotes | join (' ') }})
{% endif %}
{% if backup.keep_daily is defined %}
KEEP_DAILY={{ backup.keep_daily }}
{% endif %}
{% if backup.keep_weekly is defined %}
KEEP_WEEKLY={{ backup.keep_weekly }}
{% endif %}
{% if backup.keep_monthly is defined %}
KEEP_MONTHLY={{ backup.keep_monthly }}
{% endif %}
{% if backup.keep_yearly is defined %}
KEEP_YEARLY={{ backup.keep_yearly }}
{% endif %}

{% endif %}
{% if variables is defined %}
{% for variable in variables %}
{{ variable }}
{% endfor %}
{% endif %}